function on_event(EventType event, int data) {

    if (event==eEventEnterRoomBeforeFadein){

        musique=(music_slider.Value);
        if (musique<=17)      SetMusicVolume(-3);
        else if (musique<=34) SetMusicVolume(-2);
        else if (musique<=50) SetMusicVolume(-1);
        else if (musique<=68) SetMusicVolume(0);
        else if (musique<=85) SetMusicVolume(1);
        else if (musique<=95) SetMusicVolume(2);
        else                  SetMusicVolume(3);
        SetMusicMasterVolume((music_slider.Value));

        offtoschooled=false;
        camehomed=false;
        is_beccabreakfasting=false;
        laura_autoasked=false;

        //background frame 1: dark - frame 2: light
        if ((ampm==0) && ((hour>=1) && (hour<8)))  //Early Morning 01:00AM-07.59AM
            SetBackgroundFrame(1);
        if ((ampm==0) && (hour>=8) && (hour!=12))  //Morning 08:00AM-11:59AM
            SetBackgroundFrame(0);
        if ((ampm==1) && (hour==12))  //Noon 12:00PM-12:59PM
            SetBackgroundFrame(0);
        if ((ampm==1) && (hour<=8))  //Day 01:00PM-08:59PM
            SetBackgroundFrame(0);
        if ((ampm==1) && (hour>=9)&&(hour!=12))  //Night 09:00PM-11:59PM
            SetBackgroundFrame(1);
        if ((ampm==0) && (hour==12))  //Midnight 12:00AM-12:59AM
            SetBackgroundFrame(1);

        SetTimer(1, 40);

        lauratalkable=true; laura_initiated=false;
        Laura.Transparency=0; Rebecca.Transparency=0;
        Laura.Baseline=0; Rebecca.Baseline=0;  //Resetting this here

        if (player.PreviousRoom==29)  //Dream sequence
            {player.ChangeView(80); player.SetIdleView(81, 0);}  //Wakes up in PJs

        if (melplot==0){  //Starting out
            Laura.ChangeRoom(4, 100, 100); Rebecca.ChangeRoom(2, 282,  58);
        }

        if (melplot==1){  //Before school ends on Monday.
            if ((milhour<=6) || (milhour>=20)){  //Evening to early morning (20:00-05:59)
                Rebecca.ChangeRoom(2, 310, 125); Rebecca.Baseline=10; Rebecca.Animate(0, 3, eRepeat, eNoBlock);
            }
            if (milhour==7){  //Seven in the morning
                Rebecca.ChangeRoom(2, 170, 192); Rebecca.Animate(5, 3, eRepeat, eNoBlock);
            }
            if ((milhour>7) && (milhour<16)){  //Daytime
                Rebecca.ChangeRoom(24, 10, 10); Rebecca.Animate(0, 3, eRepeat, eNoBlock);
            }
            if ((milhour>16) && (milhour<20)){  //Afternoon
                Rebecca.ChangeRoom(2, 310, 125); Rebecca.Animate(0, 3, eRepeat, eNoBlock);
            }
        }

        if (melplot==99){  //Mel didn't pick up Laura
            Rebecca.ChangeRoom(2, 380, 192); Rebecca.Animate(0, 3, eRepeat, eNoBlock);
            Laura.ChangeRoom(24, 222, 118); Laura.ChangeRoom(24, 10, 10);
        }

        if ((hour>=7) && (hour<=11) && (!mel_lostcustody) && (player.PreviousRoom==29)){  //Becca wakes Melanie
            salebuyer=("Rebecca");
            vitality.Width-=((hour-6)*10);  //Punish-mints: drop 10 vitality points for each hour after 6
            hour=6; ampm=0; milhour=6; minute=35;  //TimeSet
            is_beccabreakfasting=true;
        }

        /* Regular starts */
        if ((melplot>=2) && (melplot!=99)){  //After school ends on Monday.
            if ((milhour<=6) || (milhour>=20)){  //Evening to early morning (20:00-05:59)
                Laura.ChangeRoom(2, 222, 59); Rebecca.ChangeRoom(2, 310, 125); Rebecca.Baseline=10; Rebecca.Animate(0, 3, eRepeat, eNoBlock);
            }
            if (milhour==7){
                if (minute<30){  //Seven, Early
                    Laura.ChangeRoom(2, 222, 59); Rebecca.ChangeRoom(2, 170, 192); Rebecca.Animate(5, 3, eRepeat, eNoBlock);}
                if (minute>=30){  //Seven, Late
                    Laura.SetWalkSpeed(10, 10); Laura.ChangeRoom(2, 370, 192); Rebecca.ChangeRoom(2, 192, 192); Rebecca.Animate(5, 3, eRepeat, eNoBlock);}
            }
            if ((milhour>7) && (milhour<16)){  //Daytime FIXME credits room??
                Laura.ChangeRoom(24, 222, 118); Rebecca.ChangeRoom(24, 10, 10);
            }
            if ((milhour==16) || (milhour==17)){  //Afternoon 16:00-17:59
                Laura.ChangeRoom(2, 370, 192); Rebecca.ChangeRoom(24, 10, 10);
            }
            if ((milhour>17) && (milhour<20)){  //Late afternoon 18:00-19:59
                Laura.ChangeRoom(2, 260, 125); Rebecca.ChangeRoom(2, 310, 125); Rebecca.Animate(0, 3, eRepeat, eNoBlock);
            }
        }

        if ((firstdayhome) && (melplot!=99) && (dayofweek>=1)){  //First day walking Laura Home
            Laura.ChangeRoom(2, 370, 192); Rebecca.ChangeRoom(24, 10, 10); }

        player.Transparency = 0;
        player.ManualScaling = true;
        player.Scaling = 95;
        if (Rebecca.Room==2){
            Rebecca.ManualScaling = true; Rebecca.Scaling = 90;
        }
        if (Laura.Room==2){
            Laura.ManualScaling = true; Laura.Scaling = 90;
        }
        leave.Transparency=99;
        NatMusic(9);

        //Just making sure.
        if (mel_lostcustody)
            Laura.ChangeRoom(0);
        if (dayspassed==-1)
            Laura.ChangeRoom(0);
        if ((dayspassed==0) && (hour<=16))
            Laura.ChangeRoom(0);

        if (is_beccabreakfasting){
            Rebecca.x=player.x+10; Rebecca.y=player.y; Rebecca.Baseline=0;
        }

        SetTimer(4, 0);
        if ((!mel_lostcustody) && (day=="Saturday"))  //Seth's here to pick up Laura
            SetTimer(4, 200);
  
        ///aaaannnnd the blocks.
        block_1.Visible=true; block_1.Transparency=0;
        block_2.Visible=true; block_2.Transparency=0;
        block_3.Visible=true; block_3.Transparency=0;
        if (player.y<60)
            block_3.Visible=false;
        else if (player.y<140)
            block_2.Visible=false;
        else
            block_1.Visible=false;
    }
}


function lauratalk() {  // Laura dialogue

    StrLauraTexts = new String[11];
    StrLauraTexts[0]="...";
    StrLauraTexts[1]="What, you're coming with?";
    StrLauraTexts[2]="Let's go already.";
    StrLauraTexts[3]="Ready to go, finally?";
    StrLauraTexts[4]="Ready to go?";
    StrLauraTexts[5]="Hey, mom - you ready to go?";
    StrLauraTexts[6]="Think we'll meet anybody new on the way there?";
    StrLauraTexts[7]="Let's go right now so I'm not late!";
    StrLauraTexts[8]="Think it'll be a good day, today? Come on, let's go!";
    StrLauraTexts[9]="Ready when you are, mom!";
    StrLauraTexts[10]="Nice day for a walk, isn't it?";

    if (lauratalkable){  //FIXME improve distance check
        if (( ((Laura.x - player.x) <= 60) && (Laura.x - player.x) >= 0)&& (Laura.Room==player.Room)&&( ((Laura.y-player.y)<= 5)&&(Laura.y-player.y)>=-5)){//in Range
            Laura.StopMoving();
            salebuyer=("Laura"); customerlisten(); PCListen();
            TalkPop(); Wait(40);
            customerlisten(); PCListen(); dName.Text=(" "); dText.Text=(" ");
            if (milhour==7){  //time to go to school
                customertalk(); PCListen();
                if (approval_laura<=0)
                    dText.Text=StrLauraTexts[0];
                else if (approval_laura<6)
                    dText.Text=StrLauraTexts[approval_laura];  //answer related to approval
                else
                    dText.Text=StrLauraTexts[6+Random(4)];  //random happy answer if approval>6
                blab1(); walkscene_direction=0;
                customerlisten(); dText.Text=(" ");
                dLauraHeadout.Start();
            }
            else {  // any other hour
                customertalk(); PCListen();
                if(approval_laura<=0)
                    dText.Text=("...");
                else
                    switch(approval_laura) {
                        case 1: dText.Text=("What?");
                                break;
                        case 2: dText.Text=("Hi, mom.");
                                break;
                        case 3: dText.Text=("Hey, mom.");
                                break;
                        case 4: dText.Text=("Hey-hey, momma-momma.");
                                break;
                        case 5: dText.Text=("Shoop-a-doop-doop.");
                                break;
                        default:dText.Text=("La la la la.");
                                break;
                    }
                blab1();
                customerlisten(); dText.Text=(" ");
                StopPop(); Wait(40);
            }
        }
    }
}

